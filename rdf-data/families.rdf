<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:cultures="http://example.org/cultures#">

    <!-- Définition de la classe Famille -->
    <owl:Class rdf:about="http://example.org/cultures#Family">
        <rdfs:label xml:lang="fr">Famille de Cultures</rdfs:label>
    </owl:Class>

    <!-- Familles de cultures -->
    <cultures:Family rdf:about="http://example.org/cultures#Poacees">
        <rdfs:label xml:lang="fr">Poacées (Graminées)</rdfs:label>
        <rdfs:comment xml:lang="fr">Famille des graminées, incluant les céréales</rdfs:comment>
    </cultures:Family>

    <cultures:Family rdf:about="http://example.org/cultures#Fabacees">
        <rdfs:label xml:lang="fr">Fabacées (Légumineuses)</rdfs:label>
        <rdfs:comment xml:lang="fr">Famille des légumineuses, plantes fixatrices d'azote</rdfs:comment>
    </cultures:Family>

    <cultures:Family rdf:about="http://example.org/cultures#Musacees">
        <rdfs:label xml:lang="fr">Musacées</rdfs:label>
